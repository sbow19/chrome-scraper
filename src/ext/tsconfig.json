{
    "compilerOptions": {
      "target": "ESNext",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
  
      
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noFallthroughCasesInSwitch": true,
  
  
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      // "noEmit": true,
      "jsx": "react-jsx",
  
      "baseUrl": ".",
      "paths": {},
  
      "rootDir": ".",
      "outDir": "../../public/ext",
  
      "typeRoots": [
        "../../node_modules/@types",
       "../project_types/"
      ]
    },
    "include": [
      "../project_types/**/*",
      "**/*.ts",
      "**/*.tsx"]

  }