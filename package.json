{
	"name": "chrome-scraper",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@reduxjs/toolkit": "^2.2.7",
		"@testing-library/jest-dom": "^5.17.0",
		"@testing-library/user-event": "^13.5.0",
		"@types/chrome": "^0.0.269",
		"@types/node": "^16.18.105",
		"@types/react-redux": "^7.1.33",
		"@types/react-router-dom": "^5.3.3",
		"chrome-types": "^0.1.297",
		"idb": "^8.0.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-icons": "^5.3.0",
		"react-redux": "^9.1.2",
		"react-router-dom": "^6.26.0",
		"react-scripts": "5.0.1",
		"redux-thunk": "^3.1.0",
		"typescript": "^4.9.5",
		"web-vitals": "^2.1.4"
	},
	"scripts": {
		"start": "webpack serve --config webpack/webpack.config.js --env env=dev --open",
		"build-ext": "webpack --config webpack/webpack.config.js --env env=prod",
		"build-tsc": "tsc",
		"test": "jest"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"imports": {
		"#components/*": [
			"./src/app/components/*"
		],
		"#ducks/*": [
			"./src/app/ducks/*"
		],
		"#styles/*": [
			"./src/app/styles/*"
		],
		"#models/*": [
			"./src/app/models/*"
		],
		"#assets/*": [
			"./src/assets/*"
		],
		"#containers/*": [
			"./src/app/containers/*"
		],
		"#mocks/*": [
			"./src/__mocks__/*"
		]
	},
	"devDependencies": {
		"@babel/core": "^7.25.2",
		"@babel/preset-env": "^7.25.3",
		"@babel/preset-react": "^7.24.7",
		"@babel/preset-typescript": "^7.24.7",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/react": "^16.0.0",
		"@types/jest": "^29.5.12",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^8.1.0",
		"@typescript-eslint/parser": "^8.1.0",
		"css-loader": "^7.1.2",
		"eslint": "^8.57.0",
		"eslint-plugin-react": "^7.35.0",
		"eslint-plugin-react-hooks": "^4.6.2",
		"fake-indexeddb": "^6.0.0",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^5.6.0",
		"husky": "^9.1.4",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"lint-staged": "^15.2.8",
		"style-loader": "^4.0.0",
		"ts-jest": "^29.2.4",
		"ts-node": "^10.9.2",
		"webpack": "^5.93.0",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^5.0.4",
		"webpack-merge": "^6.0.1"
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,json, css,scss,md}": [
			"prettier --write"
		],
		"src/**/*.(js,jsx,ts,tsx,json": [
			"eslint --fix"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	}
}
